<p id="notice"><%= notice %></p>

<h1>Listing Shipping Table Rates</h1>

<table>
  <thead>
    <tr>
      <th>Country</th>
      <th>A</th>
      <th>B</th>
      <th>C</th>
      <th>D</th>
      <th>E</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @shipping_table_rates.each do |shipping_table_rate| %>
      <tr>
        <td><%= shipping_table_rate.country %></td>
        <td><%= shipping_table_rate.a %></td>
        <td><%= shipping_table_rate.b %></td>
        <td><%= shipping_table_rate.c %></td>
        <td><%= shipping_table_rate.d %></td>
        <td><%= shipping_table_rate.e %></td>
        <td><%= link_to 'Show', shipping_table_rate %></td>
        <td><%= link_to 'Edit', edit_shipping_table_rate_path(shipping_table_rate) %></td>
        <td><%= link_to 'Destroy', shipping_table_rate, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Shipping table rate', new_shipping_table_rate_path %>
